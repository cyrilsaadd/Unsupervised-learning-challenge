    "        if df.iloc[:, i].equals(df.iloc[:, j]):\n",
    "            dup_cols.append((df.columns[i], df.columns[j]))\n",
    "\n",
    "print(\"Colonnes identiques :\", dup_cols)\n",
    "\n",
    "# --- Gestion des colonnes avec suffixes (_x / _y) ---\n",
    "dup_suffix = [col for col in df.columns if col.endswith(('_x', '_y'))]\n",
    "print(\"Colonnes avec suffixe :\", dup_suffix)\n",
