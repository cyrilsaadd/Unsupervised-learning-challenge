    "import pandas as pd\n",
    "\n",
    "# --- Harmonisation basique des noms de colonnes\n",
    "def clean_names(df):\n",
    "    df.columns = df.columns.str.strip().str.lower().str.replace(' ', '_')\n",
    "    if 'player_name' in df.columns:\n",
    "        df['player_name'] = df['player_name'].str.strip().str.lower()\n",
    "    if 'club' in df.columns:\n",
    "        df['club'] = df['club'].str.strip().str.lower()\n",
    "    return df\n",
    "\n",
    "datasets = [attack_ds, attempts_ds, defend_ds, disciplinary_ds,\n",
    "             distributon_ds, goalkeeping_ds, goals_ds, key_ds]\n",
    "\n",
    "datasets = [clean_names(d) for d in datasets]\n",
    "\n",
    "# --- Fusion sur des clés simples et cohérentes\n",
    "df = datasets[0]\n",
    "for other in datasets[1:]:\n",
    "    df = df.merge(other, on=['player_name', 'club'], how='outer')\n"
